.container.span12#companies-main{ id: 'company-container', 'ng-controller' => 'company_controller' }
  .row#financials
    %h3= 'Financials'
    %ul.no-style
      %li= "Price: {{company.price}}"
      %li= "52-week high: {{company.52high}}"
      %li= "52-week low: {{company.52low}}"
      %li= "Cash: {{company.cash}}"
      %li= "Market Cap: {{company.market_cap}}"
      %li= "Shares Out: {{company.shares}}"
      %li= "Burn: {{company.burn}}"
      %li= "Runway: {{company.runway}}"
  .row#timeline
    %h3= 'Timeline'
    %ul.no-style
      %li
  .row#diligence
  .row#SEC


